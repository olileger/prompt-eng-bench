<!-- views/index.ejs -->
<!DOCTYPE html>
<html lang="en">


<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Prompt Engineering Bench YeY !</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>


<body>

  
  <div class="container mt-5 w-50">
    <div class="mb-3">
      <label for="endpoint" class="form-label">Endpoint</label>
      <input type="text" class="form-control" id="endpoint">
    </div>
    <div class="mb-3">
      <label for="key" class="form-label">Key</label>
      <input type="password" class="form-control" id="key">
    </div>
    <div class="mb-3">
      <label for="model" class="form-label">Model</label>
      <select class="form-select" id="model">
        <option value="model1">Model 1</option>
        <option value="model2">Model 2</option>
        <option value="model3">Model 3</option>
      </select>
    </div>
    <div class="mb-3">
      <label for="readonly-text" class="form-label"></label>
      <textarea class="form-control" id="readonly-text" rows="25" readonly></textarea>
    </div>
    <div class="mb-3">
      <label for="editable-text" class="form-label"></label>
      <textarea class="form-control" id="editable-text" rows="5"></textarea>
    </div>
  </div>


  <div class="container">
    <div class="row">
      <div class="col-6">
        <div>
          <label for="endpoint" class="form-label">Endpoint</label>
          <input type="text" class="form-control" id="endpoint">
        </div>
        <div>
          <label for="key" class="form-label">Key</label>
          <input type="password" class="form-control" id="key">
        </div>
        <div>
          <label for="model" class="form-label">Model</label>
          <select class="form-select" id="model">
            <option value="model1">Model 1</option>
            <option value="model2">Model 2</option>
            <option value="model3">Model 3</option>
          </select>
        </div>
        <div>
          <label for="readonly-text" class="form-label"></label>
          <textarea class="form-control" id="readonly-text" rows="25" readonly></textarea>
        </div>
        <div>
          <label for="editable-text" class="form-label"></label>
          <textarea class="form-control" id="editable-text" rows="5"></textarea>
        </div>
      </div>
    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>