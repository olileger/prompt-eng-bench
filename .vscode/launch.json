{
    "version": "0.2.0",
    "configurations":
    [
        {
            "type": "firefox",
            "request": "launch",
            "name": "vuejs: firefox",
            "preLaunchTask": "npm: serve",
            "postDebugTask": "npm: stop serve",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/src",
            "pathMappings":
            [
                {
                    "url": "webpack://prompt-eng-bench/src",
                    "path": "${workspaceFolder}/src"
                }
            ]
        }
    ]
}